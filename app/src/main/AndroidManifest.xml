<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shirley.tupesoideal_final">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tupesoidealicon2"
        android:label="Tu peso ideal"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SistemaAnglosajonActivity" android:parentActivityName=".MainActivity" />
        <activity android:name=".SistemaInternacionalActivity" android:parentActivityName=".MainActivity" />
        <activity android:name=".ResultadoAnglosajonActivity" android:parentActivityName=".SistemaAnglosajonActivity" />
        <activity android:name=".ResultadoInternacionalActivity" android:parentActivityName=".SistemaInternacionalActivity"></activity>
    </application>

</manifest>